    <header>
      <section class="container">
        <div id="site-logo">
          <a href="https://www.autoabonnement.nl/">
            <amp-img src="https://www.autoabonnement.nl/-/media/images/autoabonnement/logo/logo_autoabonnement.png" title="header" alt="header" layout="responsive" height="37" width="234"></amp-img>
          </a>
        </div>
      </section>
    </header>
    <main>
        <hr>
        <section id="pricingOptions" class="show" [class]="!props.appointmentFormVisible ? 'show' : 'hide'">
            <section class="container">
                <h1 class="title-font">Gaudi A4 Limousine</h1>
            </section>
            <section class="container">

                <amp-carousel id="carousel-with-preview" width="700" height="251" layout="responsive" type="slides">
                    <amp-img [src]="(props[currentColour].images[0] || props[props.defaultColour].images[0])"
                             src="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/audi-a5-sb-tangorood-1.jpg"
                             [srcset]="(props[currentColour].images[0] || props[props.defaultColour].images[0])"
                             srcset="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/audi-a5-sb-tangorood-1.jpg"
                             width="700" height="251" layout="responsive" alt=""></amp-img>
                    <amp-img [src]="props[currentColour].images[1] || props[props.defaultColour].images[1]"
                             src="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/audi-a5-sb-tangorood-2.png"
                             [srcset]="props[currentColour].images[1] || props[props.defaultColour].images[1]"
                             srcset="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/audi-a5-sb-tangorood-2.png"
                             width="700" height="251" layout="responsive" alt=""></amp-img>
                    <amp-img [src]="props[currentColour].images[2] || props[props.defaultColour].images[2]"
                             src="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/audi-a5-sb-tangorood-3.png"
                             [srcset]="props[currentColour].images[2] || props[props.defaultColour].images[2]"
                             srcset="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/audi-a5-sb-tangorood-3.png"
                             width="700" height="251" layout="responsive" alt=""></amp-img>
                    <amp-img [src]="props[currentColour].images[3] || props[props.defaultColour].images[3]"
                             src="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/a5-interieur-1.jpg"
                             [srcset]="props[currentColour].images[3] || props[props.defaultColour].images[3]"
                             srcset="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/a5-interieur-1.jpg"
                             width="700" height="251" layout="responsive" alt=""></amp-img>
                    <amp-img [src]="props[currentColour].images[4] || props[props.defaultColour].images[4]"
                             src="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/a5-interieur-2.jpg"
                             [srcset]="props[currentColour].images[4] || props[props.defaultColour].images[4]"
                             srcset="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/a5-interieur-2.jpg"
                             width="700" height="251" layout="responsive" alt=""></amp-img>
                </amp-carousel>
                <div class="carousel-preview">
                    <div role="button" tabindex="1" on="tap:carousel-with-preview.goToSlide(index=0)">
                        <amp-img [src]="props[currentColour].images[0] || props[props.defaultColour].images[0]"
                                 src="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/audi-a5-sb-tangorood-1.jpg"
                                 width="60" height="40" layout="responsive" alt=""></amp-img>
                    </div>
                    <div role="button" tabindex="2" on="tap:carousel-with-preview.goToSlide(index=1)">
                        <amp-img [src]="props[currentColour].images[1] || props[props.defaultColour].images[1]"
                                 src="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/audi-a5-sb-tangorood-2.png"
                                 width="60" height="40" layout="responsive" alt=""></amp-img>
                    </div>
                    <div role="button" tabindex="3" on="tap:carousel-with-preview.goToSlide(index=2)">
                        <amp-img [src]="props[currentColour].images[2] || props[props.defaultColour].images[2]"
                                 src="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/audi-a5-sb-tangorood-3.png"
                                 width="60" height="40" layout="responsive" alt=""></amp-img>
                    </div>
                    <div role="button" tabindex="4" on="tap:carousel-with-preview.goToSlide(index=3)">
                        <amp-img [src]="props[currentColour].images[3] || props[props.defaultColour].images[3]"
                                 src="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/a5-interieur-1.jpg"
                                 width="60" height="40" layout="responsive" alt=""></amp-img>
                    </div>
                    <div role="button" tabindex="5" on="tap:carousel-with-preview.goToSlide(index=4)">
                        <amp-img [src]="props[currentColour].images[4] || props[props.defaultColour].images[4]"
                                 src="https://www.autoabonnement.nl/-/media/images/autoabonnement/shop/car-colours/audi/a5/a5-sportback/tangorood-metallic/a5-interieur-2.jpg"
                                 width="60" height="40" layout="responsive" alt=""></amp-img>
                    </div>
                </div>
            </section>
            <section class="container">
                <hr>
                <div>
                    <h3 class="title-font">
                        Kleur Keuze
                    </h3>
                    <div class="colour-selection">
                        <div role="button" tabindex="1"
                             on="tap:AMP.setState({currentColour: 'briljantzwart'}),carousel-with-preview.goToSlide(index=0)"
                             class="colour-picker briljantzwart"></div>
                        <div role="button" tabindex="2"
                             on="tap:AMP.setState({currentColour: 'tangoroodMetallic'}),carousel-with-preview.goToSlide(index=0)"
                             class="colour-picker tangorood-metallic"></div>
                        <div role="button" tabindex="3"
                             on="tap:AMP.setState({currentColour: 'scubablauwMetallic'}),carousel-with-preview.goToSlide(index=0)"
                             class="colour-picker scubablauw-metallic"></div>
                    </div>
                </div>
                <div class="selected-color-preview">
                    <div class="tangorood-metallic" [class]="props[currentColour].colourClassName || props[props.defaultColour].colourClassName"></div>
                    <span [text]="props[currentColour].fullColourName || props[props.defaultColour].fullColourName"
                          class="title-font selected-color-preview">Tangorood Metallic</span>
                    <span class="additonal-pricing-font" [text]="'+ € ' + (props[currentColour].pricePerMonth == 0 ? 0 / 100 : (props[currentColour].pricePerMonth || props[props.defaultColour].pricePerMonth)) + ' p/m'">+ € 16.01 p/m</span>
                </div>
                <hr>
            </section>
            <section class="container">
                <div class="amount-selector">
                    <h3 class="title-font">
                        Kilometers per jaar
                    </h3>
                    <select class="select-list" on="change:AMP.setState({ selectedKilometrage: event.value })">
                        <option selected value=10000 label="10000 km">10000 km</option>
                        <option value=15000 label="15000 km">15000 km</option>
                        <option value=20000 label="20000 km">20000 km</option>
                        <option value=25000 label="25000 km">25000 km</option>
                        <option value=30000 label="30000 km">30000 km</option>
                    </select>
                </div>
                <div class="amount-selector">
                    <h3 class="title-font">
                        Looptijd
                    </h3>
                    <select class="select-list" on="change:AMP.setState({ selectedDuration: event.value })">
                        <option selected value=24 label="24 Maanden">24 Maanden</option>
                        <option value=36 label="36 Maanden">36 Maanden</option>
                        <option value=48 label="48 Maanden">48 Maanden</option>
                    </select>
                </div>
                <hr>
            </section>
            <section class="container">
                <div class="total-amount">
                    <h3 class="title-font">
                        Totaalbedrag
                    </h3>
                    <span class="additonal-pricing-font" [text]="'€ ' + leasePriceState.totalLeasePrice + ' p/m'">€ 869.02 p/m</span>
                </div>
            </section>
            <section class="container">
                <div class="button-orange" on="tap:AMP.setState({props: {appointmentFormVisible: true, dealDetailsVisible: false}})" role="button" tabindex="1">
                    <span>Kies deze uitvoering</span>
                    <!--<a href="https://www.autoabonnement.nl/private-lease/Audi/A5-sportback/Special/overzicht?Duration=24-maanden&Mileage=10-000-km&SelectedOptions=winterbanden&SelectedInsurances=all-risk,schadeverzekering-inzittenden&SelectedPolicyExcessOption=-250-20&SelectedColour=8a0f36a1-bc79-4813-b82c-699c00fc6e82&FuelType=benzine&TransmissionType=handgeschakeld&SelectedBrand=Audi"
                   [href]="'https://www.autoabonnement.nl/private-lease/Audi/A5-sportback/Special/overzicht?Duration=' + (selectedDuration || '24') + '-maanden&Mileage=' + ((selectedKilometrage / 1000) || 10) + '-000-km&SelectedOptions=winterbanden&SelectedInsurances=all-risk,schadeverzekering-inzittenden&SelectedPolicyExcessOption=-250-20&SelectedColour=' + (props[currentColour].id || props[props.defaultColour].id) + '&FuelType=benzine&TransmissionType=handgeschakeld&SelectedBrand=Audi'" target="_self">
                    Kies deze uitvoering
                </a>-->
                </div>
            </section>
        </section>
        <section id="dealerAppointment" class="hide container" [class]="props.appointmentFormVisible ? 'show container' : 'hide container'">
            <div class="show" [class]="!props.dealDetailsVisible ? 'show' : 'hide'">
                <div class="dealer-info-field">
                    <h2>Private Lease overzicht</h2>
                    <p>Vraag vrijblijvend een proefrit of offerte aan door onderstaand formulier in te vullen. De dealer zal zo snel mogelijk contact opnemen. Geen zorgen, je zit nog nergens aan vast!</p>
                </div>
                <form id="appointment-form"
                      method="post"
                      action-xhr="/api/carDealers/appointment"
                      custom-validation-reporting="show-all-on-submit"
                      on="submit-success:AMP.setState({props: {dealDetailsVisible: true}})"
                      target="_top">
                    <div class="dealer-info-field">
                        <p>Aanhef</p>
                        <span>
                            <input name="Aanhef"
                                   value="Dhr."
                                   type="radio">Dhr.
                        </span>
                        <span>
                            <input name="Aanhef"
                                   value="Mevr."
                                   type="radio">Mevr.
                        </span>
                    </div>

                    <div class="dealer-info-field">
                        <label for="firstName">Voornaam</label>
                        <input type="text"
                               name="firstName"
                               id="firstName"
                               placeholder="Voornaam"
                               required>
                        <div class="error-container" visible-when-invalid="valueMissing" validation-for="firstName"></div>
                    </div>
                    <div class="dealer-info-field">
                        <label for="mainText">Tussenvoegsel</label>
                        <input type="text"
                               name="mainText"
                               id="mainText"
                               placeholder="Tussenvoegsel">
                    </div>
                    <div class="dealer-info-field">
                        <label for="lastName">Achternaam</label>
                        <input type="text"
                               name="lastName"
                               id="lastName"
                               placeholder="Achternaam"
                               required>
                        <div class="error-container" visible-when-invalid="valueMissing" validation-for="lastName"></div>
                    </div>
                    <div class="dealer-info-field">
                        <label for="email">Email</label>
                        <input type="email"
                               name="email"
                               id="email"
                               placeholder="Email"
                               required>
                        <div class="error-container" visible-when-invalid="valueMissing" validation-for="email"></div>
                        <div class="error-container" visible-when-invalid="typeMismatch" validation-for="email"></div>
                    </div>
                    <div class="dealer-info-field no-spinners">
                        <label for="appointmentDate">Afspraakdatum</label>
                        <input name="appointmentDate"
                               type="date"
                               id="appointmentDate"
                               placeholder="Afspraakdatum"
                               required
                               value="" />
                        <div class="error-container" visible-when-invalid="valueMissing" validation-for="appointmentDate"></div>
                        <div class="error-container" visible-when-invalid="typeMismatch" validation-for="appointmentDate"></div>
                    </div>
                    <div class="dealer-info-field">
                        <label for="postCode">Postcode</label>
                        <input type="text"
                               name="postCode"
                               id="postCode"
                               placeholder="Postcode">
                    </div>
                    <div class="dealer-info-field">
                        <label for="houseNumber">Huisnummer</label>
                        <input type="text"
                               name="houseNumber"
                               id="houseNumber"
                               placeholder="Huisnummer">
                    </div>
                    <div class="dealer-info-field">
                        <label for="additiveText">Toevoeging</label>
                        <input type="text"
                               name="additiveText"
                               id="additiveText"
                               placeholder="Toevoeging">
                    </div>
                    <amp-list src="/api/carDealers" width="auto" height="38px" layout="fixed-height">
                        <template type="amp-mustache">
                            <label for="dealerName">Dealer</label>
                            <select required id="dealerName" name="dealerName" class="select-list">
                                <option value="">Selecteer een dealer</option>
                                {{#dealerNames}}
                                <option value="{{.}}">{{.}}</option>
                                {{/dealerNames}}
                            </select>
                        </template>
                    </amp-list>
                    <div class="error-container" visible-when-invalid="valueMissing" validation-for="dealerName"></div>
                    <div on="tap:appointment-form.submit" role="button" tabindex="1" class="button-orange">
                        <span>Aanvragen via de dealer</span>
                    </div>                   
                    <div submit-error class="dealer-info-field">
                        <template type="amp-mustache">
                            An error occured while sending an appointment email
                        </template>
                    </div>
                </form>
            </div>
            
            <section class="hide container" [class]="props.dealDetailsVisible ? 'show container' : 'hide container'">
                <div id="pricingOverview">
                    <div class="dealer-info-field submit-success">
                        An appointment was arranged!
                    </div>
                    <div class="car-info-section">
                        <div class="header">
                            <div>
                                <p>
                                    Auto:
                                </p>
                            </div>
                        </div>
                        <div class="content">
                            <p>
                                <span>Model:</span>
                                <span>Gaudi A4 Limousine</span>
                            </p>
                            <p>
                                <span>Kleur:</span>
                                <span [text]="props[currentColour].fullColourName || props[props.defaultColour].fullColourName"
                                      class="title-font selected-color-preview">Tangorood Metallic</span>
                                <span class="additonal-pricing-font pull-right" [text]="'€ ' + (props[currentColour].pricePerMonth == 0 ? 0 / 100 : (props[currentColour].pricePerMonth || props[props.defaultColour].pricePerMonth)) + ' p/m'">€ 16.01 p/m</span>
                            </p>
                        </div>
                    </div>
                    <div class="car-info-section">
                        <div class="header">
                            <div>
                                <p>
                                    Contract:
                                </p>
                            </div>
                        </div>
                        <div class="content">
                            <p>
                                <span>Looptijd:</span>
                                <span [text]="(selectedDuration || 24) + ' Maanden'">24 Maanden</span>
                            </p>
                            <p>
                                <span>Kilometers per jaar:</span>
                                <span [text]="(selectedKilometrage || 10000) + ' km'">10000 km</span>
                            </p>
                        </div>
                    </div>
                    <div class="car-info-section">
                        <div class="header">
                            <div>
                                <p>
                                    <span>Totaalbedrag:</span>
                                    <span class="additonal-pricing-font pull-right" [text]="'€ ' + leasePriceState.totalLeasePrice + ' p/m'">€ 869.02 p/m</span>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <div on="tap:AMP.setState({ props: { appointmentFormVisible: false }})" role="button" tabindex="2" class="button-back">
                <span>Terug</span>
            </div>
        </section>
        
    </main>
  </body>
